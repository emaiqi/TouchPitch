/* pages/requirements/requirements.wxss */
page{
  background:#fff;
}
.header{
  width:100%;
  height:70px;
  background: #F1F3F7;
  border:1px solid #F1F3F7;
}
.header-content{
  width:89%;
  margin:13px auto 0px;
  font-family: PingFangSC-Regular;
  font-size: 13px;
  color: #42528D;
  letter-spacing: 0;
}
/*内容*/
.contents{
  width:100%;
  margin-bottom:25px;
  display: flex;flex-flow: row;
}
.contents .dian{
  width:6px;
  margin-left:20px;
  margin-right:11px;
  display: flex;
  flex-flow: column;
}
.contents .dian text{
   display: inline-block;
    width:6px;
  height:6px;
  margin-top:6px;
   background: #CDB05D;
}
.contents .flex-wrap{
  margin-right:20px;
  font-family: PingFangSC-Regular;
  font-size: 12px;
  color: #7E899D;
  letter-spacing: 0;
  line-height: 17px;
}