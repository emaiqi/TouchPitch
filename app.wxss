/**app.wxss**/
@import 'style/weui.wxss';
.container{
  width:100%;
  overflow-x: hidden
}
page{
    background-color: #F3F3F3;
    font-size: 16px;
    font-family: PingFangSC-Regular;
}
.page__hd {
    padding: 40px;
}
.page__bd {
    padding-bottom: 40px;
}
.page__bd_spacing {
    padding-left: 15px;
    padding-right: 15px;
}

.page__ft{
    padding-bottom: 10px;
    text-align: center;
}

.page__title {
    text-align: left;
    font-size: 20px;
    font-weight: 400;
}

.page__desc {
    margin-top: 5px;
    color: #888888;
    text-align: left;
    font-size: 14px;
}
